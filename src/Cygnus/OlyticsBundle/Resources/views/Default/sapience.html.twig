<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Home{% endblock %} | Sapience</title>

        <!-- Bootstrap -->
        <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css">
        <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-theme.min.css">

        {% block stylesheets %}
        {% endblock %}

        <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
        <script type="text/javascript" src="/sapience.js"></script>

        <script type="text/javascript">
            Sapience.setDebug(true);
            Sapience.Config
                .setApp('brevity')
                .setEndpoint('/acbm/fcp')
                .setTrackerDomain('http://dev.olytics.cygnus.com')
            ;

            var sampleEntity = {type: 'content', clientId: 1234};
            Sapience.Tracker.send('view', sampleEntity);
            Sapience.Tracker.Focus.bind(sampleEntity);
            Sapience.Tracker.Scroll.bind(sampleEntity);

        </script>

        {% block javascripts %}
        {% endblock %}

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->

        {% block head %}
        {% endblock %}

    </head>

    <body>


        <div class="container-fluid">
            <div class="row">
                    {% block content %}
                    <p class="text-center">
                        <span class="label label-success">OK</span>
                    </p>
                    {% endblock %}
            </div>
            <div style="height: 4970px"></div>
        </div>

        <!-- Include all compiled plugins (below), or include individual files as needed -->
        <script type="text/javascript" src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
    </body>

</html>


